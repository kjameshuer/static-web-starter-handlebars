<header data-sticky-container id="header-main">
    <div class="" data-sticky data-options="marginTop:0;" style="width:100%">
        <div class="header grid-fluid grid-x align-center-middle text-center">
            <div class="site-identity cell small-10 medium-6 large-6">
                <h3 class="site-title medium-text-left">Static Web Starter</h3>
            </div>
            <div class="cell medium-6 large-6">
                {{> components/SocialIcons socialClass='social-header'}}
                {{> components/Nav}}
            </div>
            <div class="hide-for-medium cell small-2">
                <button type="button" class="button no-back" data-toggle="offCanvas">&#9776;</button>
            </div>
        </div>
    </div>
</header>