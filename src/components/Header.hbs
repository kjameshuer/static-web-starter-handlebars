<header data-sticky-container id="header-main">
    <div class="" data-sticky data-options="marginTop:0;" style="width:100%">

        <div class="header grid-fluid grid-x">
            <div class="cell small-12">
                <div class="title-bar" data-responsive-toggle="responsive-menu" data-hide-for="medium">
                    <button class="menu-icon" type="button" data-toggle="responsive-menu"></button>
                    <div class="title-bar-title">Menu</div>
                </div>

                <div class="top-bar" id="responsive-menu">
                    <div class="top-bar-left">
                        <h1 class="menu-title">Site Title</h1>
                    </div>
                    <div class="top-bar-right">
                        {{> components/SocialIcons socialClass='social-header'}}
                        {{> components/Nav}}
                    </div>
                </div>
            </div>
        </div>
</header>